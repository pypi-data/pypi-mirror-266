{
  "name": "ea_dash",
  "version": "0.0.15",
  "description": "Dash versions of shared React components used in Energy Aspects applications",
  "repository": {
    "type": "git",
    "url": "git://github.com/energyaspects/ea-dash.git"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/energyaspects/ea-dash/issues"
  },
  "homepage": "https://github.com/energyaspects/ea-dash",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:components": "dash-generate-components ./src/lib/components ea_dash -p package-info.json --r-prefix 'ea-dash' --jl-prefix 'ea-dash' --ignore \\.test\\. ",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:components",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Pavol Soltes <soltes.pavol@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@energyaspects/ea-design-system-react": "^4.4.1",
    "ramda": "^0.26.1",
    "react-bootstrap": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@plotly/dash-component-plugins": "^1.2.0",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-docgen": "^4.1.1",
    "react-dom": "^18.2.0",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.0.1",
    "webpack-serve": "^4.0.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  },
  "gitHead": "b3d8587a74561a365b39c2c379938862f0f317ef"
}
