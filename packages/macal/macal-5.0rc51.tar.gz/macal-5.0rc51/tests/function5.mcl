// Back to Fibonacci numbers again

fib => (n) {
    if n == 1 or n == 2 {
        return 1;
    }
    return fib(n-1) + fib(n-2);
}

print("Fibonacci for  1: ", fib(1));
print("Fibonacci for  2: ", fib(2));
print("Fibonacci for  3: ", fib(3));
print("Fibonacci for  4: ", fib(4));
print("Fibonacci for  5: ", fib(5));
print("Fibonacci for  6: ", fib(6));
print("Fibonacci for  7: ", fib(7));
print("Fibonacci for  8: ", fib(8));
print("Fibonacci for  9: ", fib(9));
print("Fibonacci for 10: ", fib(10));