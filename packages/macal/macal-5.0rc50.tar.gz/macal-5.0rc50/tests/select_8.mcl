var = [{'id': '12345', 
         'name': 'Test company', 
         'url': 'https://1', 
         'samlConsumerUrl': 'https://2', 
         'samlConsumerUrls': ['https://3'], 
         'api': {'enabled': true}, 
         'licensing': {'model': 'co-term'}, 
         'cloud': {'region': {'name': 'Europe'}}, 
         'management': {'details': []}}];

org_id = '12345';
print();
