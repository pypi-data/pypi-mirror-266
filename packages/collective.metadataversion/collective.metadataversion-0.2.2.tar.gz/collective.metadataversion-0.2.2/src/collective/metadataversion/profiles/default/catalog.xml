<?xml version="1.0"?>
<object name="portal_catalog" meta_type="Plone Catalog Tool">
  <!-- A simple integer number which allows you to keep track of the
       metadata changes; e.g., you can query your catalog and reindex
       all objects which don't have this column yet in their catalog brains,
       or have been last reindexed (with metadata, that is) with an older setup.
       Nothing magic here, really.
    -->
  <column value="metadata_version"/>
</object>

