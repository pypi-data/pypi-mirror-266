{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "traceback.json",
  "title": "Traceback",
  "description": "An error traceback.",
  "type": "object",
  "required": [
    "entries"
  ],
  "properties": {
    "entries": {
      "type": "array",
      "description": "Traceback entries.",
      "items": {
        "type": "object",
        "required": [
          "path",
          "lineno",
          "message"
        ],
        "properties": {
          "path": {
            "type": "string",
            "description": "File path."
          },
          "lineno": {
            "type": "integer",
            "description": "Line number."
          },
          "message": {
            "type": "string",
            "description": "Error message."
          }
        }
      }
    }
  }
}