{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://raw.githubusercontent.com/charbonnierg/pytest-broadcaster/main/schemas/test_case_error.json",
  "title": "Test Case Error",
  "description": "An error which occured during the execution of a test case (either setup, call or teardown).",
  "type": "object",
  "required": [
    "message"
  ],
  "properties": {
    "message": {
      "type": "string",
      "description": "Error message."
    },
    "traceback": {
      "$ref": "https://raw.githubusercontent.com/charbonnierg/pytest-broadcaster/main/schemas/traceback.json",
      "description": "Error traceback."
    }
  }
}